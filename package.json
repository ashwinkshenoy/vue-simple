{
  "name": "vuesimple",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:build": "lerna run build --stream",
    "lerna:version": "lerna version patch --force-publish --yes",
    "lerna:publish": "lerna publish from-git --yes --ignore-scripts --no-verify-access",
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test": "vue-cli-service test:unit",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "@vue/compat": "^3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.6.1",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-google-analytics": "^1.8.2",
    "@vuepress/plugin-pwa": "^1.2.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "add": "^2.0.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-plugin-vue": "^8.0.1",
    "lerna": "^4.0.0",
    "sass": "^1.32.8",
    "style-loader": "^2.0.0",
    "vuepress": "1.8.2",
    "vuepress-plugin-clean-urls": "^1.1.2",
    "vite": "^2.5.4",
    "vite-plugin-vue2": "^1.9.0",
    "vue-server-renderer": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
