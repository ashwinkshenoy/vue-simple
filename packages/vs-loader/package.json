{
  "name": "@vuesimple/vs-loader",
  "version": "1.3.31",
  "description": "A simple vue loader. Perfect for all your loader scenarios.",
  "main": "dist/vs-loader.umd.js",
  "module": "dist/vs-loader.esm.js",
  "unpkg": "dist/vs-loader.min.js",
  "browser": {
    "./sfc": "src/vs-loader.vue"
  },
  "scripts": {
    "build": "rm -rfv dist && npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vs-loader.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vs-loader.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vs-loader.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-image": "2.0.5",
    "minimist": "1.2.0",
    "rollup": "1.32.0",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-terser": "6.1.0",
    "rollup-plugin-vue": "5.1.9",
    "vue": "2.6.10",
    "vue-template-compiler": "2.6.10"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashwinkshenoy/vue-simple.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "vue",
    "vuejs",
    "loader",
    "notification",
    "front-end",
    "popup",
    "web",
    "vue-js",
    "loader",
    "vue-js-loader",
    "vue-loader",
    "vs-loader"
  ],
  "author": "Ashwin Shenoy <ashwinkshenoy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashwinkshenoy/vue-simple/issues"
  },
  "homepage": "https://github.com/ashwinkshenoy/vue-simple/tree/master/packages/vs-loader",
  "gitHead": "66f1d390d6caa9ec346508de37813f5adff15efb"
}
