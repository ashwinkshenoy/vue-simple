{
  "name": "@vuesimple/vs-datepicker",
  "version": "1.3.40",
  "description": "A simple vue datepicker. Perfect for all your datepicker scenarios.",
  "main": "dist/vs-datepicker.umd.js",
  "module": "dist/vs-datepicker.esm.js",
  "unpkg": "dist/vs-datepicker.min.js",
  "browser": {
    "./sfc": "src/vs-datepicker.vue"
  },
  "scripts": {
    "build": "rm -rfv dist && npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vs-datepicker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vs-datepicker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vs-datepicker.min.js"
  },
  "dependencies": {
    "vue2-datepicker": "^3.9.1"
  },
  "devDependencies": {
    "@rollup/plugin-image": "2.0.5",
    "minimist": "^1.2.0",
    "rollup": "^1.32.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashwinkshenoy/vue-simple.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "vue",
    "vuejs",
    "datepicker",
    "notification",
    "front-end",
    "popup",
    "web",
    "vue-js",
    "datepicker",
    "vue-js-datepicker",
    "vue-datepicker",
    "vs-datepicker"
  ],
  "author": "Ashwin Shenoy <ashwinkshenoy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashwinkshenoy/vue-simple/issues"
  },
  "homepage": "https://github.com/ashwinkshenoy/vue-simple/tree/master/packages/vs-datepicker",
  "gitHead": "1a03804c5538fbeb6c65fcf5ada83a65798f42ab"
}
